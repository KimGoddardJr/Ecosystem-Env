{
    "environment": {
        "VRAY_VERSION": "4.12",
        "VRAY_ROOT": {
            "darwin": "${PG_SW_BASE}/chaosgroup/vray_adv_41201_maya${MAYA_VERSION}_mavericks_x64", 
            "linux": "${PG_SW_BASE}/chaosgroup/vray_adv_41201_maya${MAYA_VERSION}_linux_x64", 
            "windows": "${PG_SW_BASE}/chaosgroup/vray_adv_41201_maya${MAYA_VERSION}_x64"
        }, 
        "VRAY_FOR_MAYA2018_MAIN": "${VRAY_ROOT}/maya_vray", 
        "VRAY_FOR_MAYA2018_PLUGINS": "${VRAY_ROOT}/maya_vray/vrayplugins", 
        "VRAY_OSL_PATH_MAYA2018": "${VRAY_ROOT}/vray/opensl", 
        "VRAY_FOR_MAYA2019_MAIN": "${VRAY_ROOT}/maya_vray", 
        "VRAY_FOR_MAYA2019_PLUGINS": "${VRAY_ROOT}/maya_vray/vrayplugins", 
        "VRAY_OSL_PATH_MAYA2019": "${VRAY_ROOT}/vray/opensl", 
        "VRAY_PATH": "${VRAY_ROOT}/maya_vray/bin", 
        "VRAY_PLUGINS": "${VRAY_ROOT}/maya_vray/vrayplugins", 
        "VRAY_TOOLS_MAYA2018": "${VRAY_ROOT}/vray/bin", 
        "VRAY_TOOLS_MAYA2019": "${VRAY_ROOT}/vray/bin", 
        "MAYA_ICON_PATH": "${VRAY_ROOT}/maya_vray/icons", 
        "MAYA_PLUG_IN_PATH": "${VRAY_ROOT}/maya_vray/plug-ins", 
        "MAYA_SCRIPT_PATH": "${VRAY_ROOT}/maya_vray/scripts", 
        "PATH": {
            "windows": "${VRAY_ROOT}/maya_root/bin"
        }, 
        "LD_LIBRARY_PATH": {
            "linux": "${VRAY_ROOT}/maya_root/lib"
        },
        "DYLD_LIBRARY_PATH": {
            "darwin": "${VRAY_ROOT}/maya_root/Maya.app/Contents/MacOS"
        },
        "XBMLANGPATH": {
            "darwin": "${VRAY_ROOT}/maya_vray/icons", 
            "linux": "${VRAY_ROOT}/maya_vray/icons/%B", 
            "windows": "${VRAY_ROOT}/maya_vray/icons"
        }
    }, 
    "optional": {
        "dev": {
            "VRAY_DEV_BUILDS": {
                "abs": true, 
                "common": "${DEV_BUILDS}"
            }, 
            "VRAY_FOR_MAYA2018_PLUGINS": "${VRAY_DEV_BUILDS}", 
            "VRAY_FOR_MAYA2019_PLUGINS": "${VRAY_DEV_BUILDS}", 
            "VRAY_PLUGINS": "${VRAY_DEV_BUILDS}"
        }
    }, 
    "platforms": [
        "windows", 
        "linux", 
        "darwin"
    ], 
    "requires": [
        "maya"
    ], 
    "tool": "vray", 
    "version": "4.12"
}